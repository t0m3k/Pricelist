        </main>
    <script src="https://use.fontawesome.com/f3965a4940.js"></script>
    <script src="/js/masterMain.js"></script>

    <div id="popUpMessage" class="modal fade bd-example-modal-sm" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-sm">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 id="popUpTitle" class="modal-title">Error</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <p id="popUpBody">Something went wrong. Sorry!</p>
                </div>
            </div>
        </div>
    </div>
      <% if(errorMsg && errorMsg.length > 0) { %>
                            <script>showPopUp("Error!", "<%= errorMsg %>", 3000)</script>
                <% } %>
                <% if(successMsg && successMsg.length > 0) { %>
                            <script>showPopUp("Success!", "<%= successMsg %>", 1000)</script>
                <% } %>
    </body>
</html>